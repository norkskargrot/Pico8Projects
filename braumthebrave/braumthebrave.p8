pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
#include map.lua
#include player.lua
#include camera.lua
#include entities.lua
#include draw.lua
#include physics.lua
#include projectiles.lua

function _init()
    init_entity_types()
    poke(0x5f2c, 3)
    init_map()
end

function _update60()
    update_p()
    update_room()
    update_entities()
    update_projectiles()
end

function _draw()
    set_pal()
    cls(1)
    update_cam()
    draw_map()
    draw_entities()
    draw_p()
    draw_projectiles()
    --draw_debug()

    camera()
    --print(#projectiles)
end

function draw_debug()
    rect(p.x, p.y, p.x + p.w, p.y + p.h, 5)
    for k,v in pairs(entities) do
        rect(v.x, v.y, v.x + v.w, v.y + v.h, 12)
    end
    for k,v in pairs(projectiles) do
        rect(v.x, v.y, v.x + v.w, v.y + v.h, 8)
    end
    --draw_cam_debug()
end

